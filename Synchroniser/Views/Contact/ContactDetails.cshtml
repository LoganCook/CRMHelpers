@model Client.Types.Contact

<dl class="dl-horizontal">
    <dt>Name</dt>
    <dd>@Model.Firstname @Model.Lastname</dd>
    <dt>Title</dt>
    <dd>@Model.Title</dd>
    <dt>ID</dt>
    <dd>@Model.ID</dd>
    <dt>Email</dt>
    <dd>@Model.Email</dd>
    <dt>Username</dt>
    <dd>@Model.Username</dd>
    <dt>Description</dt>
    <dd>@Model.Description</dd>
    <dt>Department</dt>
    <dd>@Model.Department</dd>
    <dt>Create on</dt>
    <dd>@Model.CreatedOn</dd>
    <dt>Phone number</dt>
    <dd>@Model.Phone</dd>
    <dt>Status</dt>
    <dd>@Model.Status</dd>
</dl>
